import java.util.*;
    
    public class Main{
        
    public static void print(long n){
        if(n==0)
            return;
        print(n/10);
        System.out.println(n%10);
    }
    
    public static void main(String[] args) {
      // write your code here  
      Scanner sc = new Scanner(System.in);
      long n = sc.nextLong();
      print(n);
     }
    }
